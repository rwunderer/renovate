{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "description": "Default preset for use with rwunderer's repos",
  "extends": [
    "config:base",
    ":preserveSemverRanges",
    ":disableMajorUpdates",
    ":automergeBranch"
  ],

  "dependencyDashboard": true,
  "prConcurrentLimit": 10,

  "vulnerabilityAlerts": {
    "labels": ["security"]
  }
}
